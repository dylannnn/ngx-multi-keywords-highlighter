        "headed": true,
        "watch": true
      },
      "configurations": {
        "ci": {
          "devServerTarget": "ngx-multi-keywords-highlighter:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/ngx-multi-keywords-highlighter-e2e/**/*.{js,ts}"

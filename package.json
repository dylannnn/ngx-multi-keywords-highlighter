  "scripts": {
    "graph": "nx graph",
    "== Lint ==": "================================================",
    "lint:all": "nx run-many --target=lint --all",
    "lint:all:fix": "nx run-many --target=lint --all --fix",
    "== Test ==": "================================================",
    "test:all": "nx run-many --target=test --all",
    "== Compodoc ==": "================================================",
    "compodoc:generate": "nx run ngx-multi-keywords-highlighter:test:coverage && npx compodoc",
    "compodoc:open": "npx compodoc --port=4405 --serve",
    "== LIBRARY ==": "================================================",
    "mkh:lint": "nx run ngx-multi-keywords-highlighter:lint",
    "mkh:test": "nx run ngx-multi-keywords-highlighter:test",
    "mkh:test:update-snapshot": "nx run ngx-multi-keywords-highlighter:test --update-snapshot=true",
    "mkh:test:watch": "nx run ngx-multi-keywords-highlighter:test:watch",
    "mkh:test:coverage": "nx run ngx-multi-keywords-highlighter:test --coverage",
    "mkh:build": "nx run ngx-multi-keywords-highlighter:build && cp README.md ./dist/libs/ngx-multi-keywords-highlighter/README.md",
    "mkh:watch": "nx run ngx-multi-keywords-highlighter:build --watch",
    "mkh:storybook": "nx run ngx-multi-keywords-highlighter:storybook",
    "mkh:storybook:build": "nx run ngx-multi-keywords-highlighter:build-storybook",
    "mkh:storybook:test": "nx run ngx-multi-keywords-highlighter:test-storybook",
    "mkh:publish": "cd dist/libs/ngx-multi-keywords-highlighter && npm publish --access public --tag <tag>",
    "mkh:unpublish": "npm unpublish @amfrontender/@amfrontender/ngx-multi-keywords-highlighter@<version>",
    "mkh:e2e": "nx run ngx-multi-keywords-highlighter-e2e:e2e",
    "mkh:e2e:lint": "nx run ngx-multi-keywords-highlighter-e2e:lint",
    "mkh:e2e:ci": "nx run ngx-multi-keywords-highlighter-e2e:e2e:ci",
    "mkh:e2e:headed": "nx run ngx-multi-keywords-highlighter-e2e:e2e:ci --headed",
    "mkh:e2e:cucumber": "nx run ngx-multi-keywords-highlighter-e2e:e2e:cucumber",
    "mkh:e2e:cucumber:watch": "nx run ngx-multi-keywords-highlighter-e2e:e2e:cucumber:watch",
    "== APP - demo ==": "================================================",
    "demo:lint": "nx lint demo",
    "demo:test": "nx test demo",
    "demo:test:watch": "nx test demo --watch",
    "demo:test:coverage": "nx test demo --coverage",
    "demo:serve": "nx serve demo",
    "demo:build": "nx build demo",
    "demo:build:stats": "nx build demo --stats-json",
    "demo:analyze": "webpack-bundle-analyzer dist/apps/demo/stats.json",
    "demo:e2e-headless": "nx e2e demo-e2e",
    "demo:e2e-heded": "nx e2e demo-e2e --headed",
    "== Cypress ==": "================================================",
    "cypress:demo:open": "cd apps/demo-e2e && npx cypress open",
    "cypress:open": "cd apps/ngx-multi-keywords-highlighter-e2e && npx cypress open",
    "== Chromatic ==": "================================================",
    "chromatic": "npx chromatic --build-script-name=mkh:storybook:build --force-rebuild --exit-zero-on-changes",
    "chromatic:exit-once-uploaded": "npx chromatic --build-script-name=mkh:storybook:build --exit-once-uploaded",
    "chromatic:exit-zero-on-changes": "npx chromatic --build-script-name=mkh:storybook:build --exit-zero-on-changes",
    "chromatic:auto-accept-changes": "npx chromatic --build-script-name=mkh:storybook:build --auto-accept-changes",
    "== Storybook ==": "================================================",
    "storybook:build": "nx run ngx-multi-keywords-highlighter:build-storybook",
    "== Changelog & Release ==": "================================================",
    "== CODE COVERAGE ==": "================================================",
    "coverage:lib:serve": "npx http-server dist/coverage/libs/ngx-multi-keywords-highlighter -p 4402",
    "coverage:demo:serve": "npx http-server dist/coverage/apps/demo -p 4403",
    "== JEST ==": "================================================",
    "jest:update:snapshot": "npx nx affected --targets=test --update-snapshot",
    "== REPORT ==": "================================================",
    "ngx-multi-keywords-highlighter:cypress:cucumber:report": "npx http-server dist/cypress/apps/ngx-multi-keywords-highlighter-e2e/cucumber",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "**/*": "npm run lint:all"
  },
